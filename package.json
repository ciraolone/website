{
  "name": "website",
  "version": "1.6.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf site",
    "build:css": "postcss src/css/main.scss -o site/css/main.css",
    "build:eleventy": "eleventy",
    "build": "npm run clean && npm-run-all -p build:*",
    "watch:css": "postcss src/css/main.scss -o site/css/main.css --watch",
    "watch:eleventy": "eleventy --serve",
    "dev": "concurrently \"npm:watch:*\"",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"**/*.{js,json,md,njk,html,css,scss}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@csstools/postcss-sass": "^5.1.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.13"
  },
  "dependencies": {
    "markdown-it-attrs": "^4.3.1"
  }
}
